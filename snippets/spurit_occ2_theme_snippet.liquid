<script>
  if(typeof(Spurit) === 'undefined'){
    var Spurit = {};
  }
  if(!Spurit.Oneclickcheckout2){
    Spurit.Oneclickcheckout2 = {};
  }
  if(!Spurit.Oneclickcheckout2.snippet){
    Spurit.Oneclickcheckout2.snippet = {};
  }
  Spurit.Oneclickcheckout2.snippet.shopHash = '94201e3a7dbe7934bfd55ae335a4355f';
  Spurit.Oneclickcheckout2.snippet.moneyFormat = '{{ shop.money_format }}';

  Spurit.Oneclickcheckout2.product = {{ product | json }}; //todo remove after 2.2 version release, if unused
  Spurit.Oneclickcheckout2.collections = {{ product.collections | json }};

  if (typeof(SPOCustom) === 'undefined') {
    SPOCustom = {};
  }
  SPOCustom.trackLineItemProperties = true;
</script>

<script src="https://s3.amazonaws.com/shopify-apps/oneclickcheckout2/common.js"></script>
<link href="https://s3.amazonaws.com/shopify-apps/oneclickcheckout2/common.css" rel="stylesheet" type="text/css" media="all">