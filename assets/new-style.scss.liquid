.icon-left-open:before{
  content: '\e819';
}
.icon-right-open:before {
  content: '\e81a';
}
.icon-cart {
  font-size: 18px;
  &:before {
    content: "\e87f";
  }
}
.input-group{
  width: 100%;
}
.main-container{ 
  .products-related{ 
    h3.title-widget {
      font-size: 17px;
      border-bottom: #e1e1e1 solid 1px;
      font-weight: 700;
      margin: 0 0 4px 0 !important;
      padding-bottom: 10px;
      text-transform: uppercase;
      text-align: left;
      color: #302e2a;
      line-height: initial;
      background: none;
      span.inner{
        background: none;
      }
      &::after{
        content: none !important;
      }
    }
    .category-products {
      .products-grid{ 
        .owl-dots {
          position: absolute;
          margin: 0;
          right: 5px;
          top: -39px;
          left: inherit;
          bottom: auto;
          .owl-dot {
            span {
              display: block;
              width: 14px;
              height: 14px;
              border-radius: 100% !important;
              border: 2px solid rgba(0,0,0,.2);
              background: none;
              position: relative;
              margin: 5px 2px 5px 2px;
              &:hover{
                opacity: 1;
                background: none !important;
                border-color: #05131c;
                &:after {
                  content: "";
                  position: absolute;
                  left: 3px;
                  top: 3px;
                  right: 3px;
                  bottom: 3px;
                  background-color: #05131c;
                  border-radius: 100%;
                }
              }
            }
            &.active {
              span {
                opacity: 1;
                background: none !important;
                border-color: #05131c;
                &:after {
                  content: "";
                  position: absolute;
                  left: 3px;
                  top: 3px;
                  right: 3px;
                  bottom: 3px;
                  background-color: #05131c;
                  border-radius: 100%;
                }
              }
            }
          }
        }
      } 
    }
    &.full-width {
      background-color: #f4f4f4;
      margin-bottom: 0;
      margin-top: 50px;
      position: relative; 
      padding: 40px 0 37px;
    }
  }
  .top-container{
    .category-banner{
      .full-width-image-banner{
        &:after{
          display: none;
        }
      }
    }
  }
  .testimonials{
    .item {
      blockquote { 
        font-size: 14px;
        font-style: normal;
        line-height: 24px;
        font-weight: 400;
        font-family: 'Open Sans';
        margin: 0 -10px;
        padding: 15px 30px 15px 43px;
        float: left;
        margin-top: -15px; 
        margin-left: 73px;
        &::before{   
          color: #65829d;
          font-family: 'porto';
          font-weight: 400;
          font-size: 54px;
        }
        &::after{
          right: 54px;   
          color: #65829d;
          font-family: 'porto';
          font-weight: 400;
          font-size: 54px;
        }
        p {
          margin-bottom: 0px;
          line-height: 24px;
          letter-spacing: 0.001em;
        }
      } 
      .testimonial-author{
        margin-bottom: 30px;
        .img-thumbnail{  
          border: none;
          padding: 0; 
        }
        p{ 
          strong {
            display: block;
            padding-top: 7px;
            text-transform: uppercase;
            font-size: 13px;
            font-weight: 700;
            letter-spacing: 0.0025em;
            color: #2b2b2d;
          }
        }
      }
    }
  }
  .main-breadcrumbs {
    font-family: Open Sans, arial, serif; 
    font-weight: 400;
    padding: 20px 0;
    line-height: 13px;
    margin-bottom: 0 !important;
  }
  .breadcrumbs {
    ul { 
      overflow: hidden;
      margin: 0; 
      li {  
        &.home a {
          width: 15px;
          display: inline-block;
          text-indent: 30px;
          overflow: hidden;
          position: relative;
          &:after {
            content: "\e883";
            font-family: 'porto';
            position: absolute;
            left: 0;
            top: 0;
            text-indent: 0;
          }
        }
      }
    }
  }
  .checkout-cart { 
    .cart-table { 
      tfoot { 
        td{ 
          a span{
            color: #777;
          }
        } 
      }
    } 
  }
  .sidebar { 
    .icon-up-dir:before {
      content: '\e81c';
    }
    .icon-down-dir:before {
      content: '\e81b';
    } 
    .block { 
      .block-content {  
        ul{
          li{
            span{ 
              &:after {
                color: #21293c;
                text-align: right;
                padding: 0;
              }
            }  
            span.expand{
              &:after{
                content:"\e874";
                font-size:11px;
              }
            }
            span.collapse{ 
              &:after{
                content:"\e873";
                font-size:11px;
              }
            }
            a{
              font-size: 14px;
              line-height: 33px;
              letter-spacing: 0.01em;
              color: #7a7d82; 
            }
          }
        }
        > ul {
          li { 
            a{
              font-size: 14px;
              line-height: 33px;
              letter-spacing: 0.01em;
              color: #7a7d82;
            }
          }
        }
      }
      .narrow-by-list { 
        dt { 
          margin-top: 0;
          margin-bottom: 0;
          padding: 20px 20px 0 20px;
          font-size: 15px;
          text-transform: uppercase;
          font-weight: 600;
          color: #21293c;
          letter-spacing: 0.01em; 
          background: none;
          border: none; 
        } 
        dd { 
          padding: 10px 20px 26px 20px;
          border-bottom: #efefef solid 1px;
          position: relative;
          z-index: 2;
          padding-bottom: 24px;
          background:none;
          border-left: none;
          border-right: none;  
          ol { 
            li { 
              font-size: 14px;
              line-height: 27px; 
            }
            &.swatch-list {  
              li { 
                .item {  
                  .has-image {                    
                    i { 
                      &.color_box { 
                        box-shadow: 0 0 10px rgba(0,0,0,0.13);
                      }
                    } 
                  }
                  &:hover,
                  &.active {
                    .has-image { 
                      i.color_box:before { 
                        text-shadow: 0 0 2px rgba(0,0,0,0.3);
                      } 
                    } 
                  }
                }  
              }
            }
          }   
        } 
      } 
      &.block-layered-nav{
        margin-bottom: 0px;
      }
    } 
    .html_sidebar {
      h2 {
        font-size: 15px !important;
        text-transform: uppercase;
        font-weight: 600;
        color: #21293c !important;
        letter-spacing: 0.01em;
        padding: 0 20px;
        margin-bottom: 15px;
      }
      h5 {
        font-family: 'Open Sans';
        font-weight: 600;
        font-size: 14px;
        color: #7a7d82;
        letter-spacing: 0.022em;
        padding: 0 20px;
      }
      p {
        color: #21293c !important;
        font-size: 15px !important;
        letter-spacing: 0.01em;
        padding: 0 20px;
      }
    }
    .sidebar-toogle, .sidebar-cate-toogle {
      .block-title { 
        .collapse {      
          border: none;
          color: #21293c;
          font-size: 17px;
          margin-top: -3px;
          z-index: 5;
          background:none; 
          i:before {
            font-weight: 700;
          }  
        }
      }
    } 
    .sidebar-cate-toogle .block-title .collapse:hover{
      background-color: transparent;
      color: #21293c;
    }
    .block-nav {
      .block-title {
        padding: 20px 20px 0 20px;
        font-size: 15px;
        text-transform: uppercase;
        font-weight: 600;
        color: #21293c;
        letter-spacing: 0.01em;
        background: none;
        border: 0; 
      } 
      .block-content { 
        border-top: 0; 
        padding: 10px 20px 26px 20px;
        border-bottom: #efefef solid 1px;
        position: relative;
        z-index: 2;
        padding-bottom: 24px;
        border-radius: 0;
        background: none;
        border-left: none;
        border-right: none;
      }
    }
    @media(min-width:992px){
      .canvas-inner { 
        border: solid 1px #dae2e6; 
      }
    }
  }
  .collection-main-banner{ 
    .category-content {
      margin-bottom: 20px;
      &.move-below-header {margin-bottom: 0;}
    }
    .full-width-image-banner{
      position:relative;
      .content{
        position: absolute;
        top: 50%;
        left: auto;
        right: auto;
        color: #fff;
        text-align: center;
        z-index: 2;
        margin-top: -36px;
        h2{ 
          margin: 0;
          font-size: 36px;
          font-weight: 900;
          letter-spacing: -0.025em;
          text-transform: uppercase;
          line-height: 38px;
          padding: 0;
        }
        p{
          font-size: 18px;
          line-height: 38px;
          font-weight: 700;
          text-transform: uppercase;
          margin-bottom: 0;
          padding: 0;
        }
        .btn-default {
          font-size: 14px;
          line-height: 25px;
          letter-spacing: 0.025em;
          padding: 10px 20px;
          background-color: #010204;
          color: #fff;
          font-family: 'Oswald';
          text-transform: uppercase;
          border-radius: 2px;
          margin-top: 31px;
          border: 0;
        }
      } 
    }  
    .full-width-image {
      text-align: center;
      padding: 200px 0;
      h3 {
        font-size: 60px;
        color: #fff;
        font-weight: 600;
        letter-spacing: -5px;
      }
      a {  
        font-weight: 600;
        line-height: 1;
        text-transform: uppercase;
        background-color: #fff;
        color: #000; 
        letter-spacing: 3px;
